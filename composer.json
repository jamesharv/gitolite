{
    "name": "webmark-james/gitolite",
    "description": "Gitter allows you to interact in an object oriented manner with Git repositories.",
    "keywords": ["git", "vcs", "gitloite"],
    "homepage": "https://github.com/webmark-james/gitolite",
    "type": "library",
    "license": "GPL-2.0+",
    "authors": [
        {
            "name": "James Harvey",
            "email": "james@webmarkdesigns.com.au"
        },
        {
            "name": "Dave Hall",
            "email": "skwashd@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "klaussilveira/gitter": "dev-master"
    },
    "minimum-stability": "stable",
    "autoload": {
        "psr-0": {
            "Gitolite": "lib/"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/skwashd/gitter"
        }
    ]
}
